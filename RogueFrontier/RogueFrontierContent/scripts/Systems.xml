						<!--Hidden warlord stronghold nearby-->
						<Orbital angleInc="-90" radius="70">
							<Station	codename="station_orion_warlords_stronghold"/>
						</Orbital>
					</Group>
					<!--small moon with ring of stations-->
					<Group radius="100" angleInc="80">
						<Planet radius="10"/>
						<Orbital radius="50" angleInc="-30" increment="20" count="5">
							<Group>
								<!--Astra + Village-->
								<At index="0,1,3,4">
									<Station	codename="station_constellation_habitat"/>
								</At>
								<At index="2">
									<Station	codename="station_constellation_astra" id="constellationAstraOrion"/>
								</At>
								<Sibling	radiusInc="15">
									<Station codename="station_constellation_habitat"/>
								</Sibling>
								
								<!--Accompanying special stations-->
								<At index="1">
									<Sibling	radiusInc="-15">
										<Station codename="station_armor_shop"/>
									</Sibling>
								</At>
								<At index="2">
									<Sibling radiusInc="-15">
										<Station codename="station_amethyst_store"/>

										<Sibling radiusInc="-15">
											<Station codename="station_constellation_missile_sentry"/>
										</Sibling>
									</Sibling>
								</At>
								<At index="3">
									<Sibling	radiusInc="-15">
										<Station codename="station_arms_dealer"/>
									</Sibling>
								</At>
							</Group>
						</Orbital>
					</Group>
					<Group radius="120" angleInc="160">
						<!--Far-away small Moon-->
						<Planet radius="8"/>
						<Orbital angle="random" radius="20">
							<Group>
								<Station	codename="station_daughters_outpost" id="daughtersOutpostOrion"/>
								<Orbital	radius="5"
											angle="random"
											>
									<!---->
									<Group>
										<Marker		name="Start"/>
										<!--
										<Ship count="4" codename="ship_iron_destroyer" sovereign="sovereign_iron_pirates"/>

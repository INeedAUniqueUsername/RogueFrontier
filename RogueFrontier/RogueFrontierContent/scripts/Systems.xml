					<Ship count="1" codename="ship_iron_frigate" sovereign="sovereign_iron_pirates"/>
					-->
				</Sibling>
			</Orbital>
			<Orbital radius="760"
					 angle="random">
				<!--Giant planet-->
				<Planet radius="20"
						showOrbit="true"
							/>
				<!--Starting area-->
				<Orbital angle="random">
					<!--tiny moon with warlord camp-->
					<Group radius="80">
						<Planet radius="5"/>
						<Orbital count="2" angle="random" increment="90-150" radius="10">
							<Station	codename="station_orion_warlords_camp"/>
